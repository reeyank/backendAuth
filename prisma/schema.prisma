generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model userInfo {
  id            Int      @id @default(autoincrement())
  created_at    DateTime @default(now()) @db.Timestamp(6)
  last_updated  DateTime @default(now()) @db.Timetz(6)
  first_name    String
  last_name     String?
  email_address String
}